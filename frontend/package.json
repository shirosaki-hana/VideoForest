  "scripts": {
    "dev": "vite",
    "build": "pnpm clean && pnpm -w exec tsc -b frontend/tsconfig.json && vite build",
    "build:debug": "pnpm clean && pnpm -w exec tsc -b frontend/tsconfig.json && vite build --mode debug",
    "lint": "pnpm -w exec eslint frontend/",
    "lint:fix": "pnpm -w exec eslint frontend/ --fix",
    "format": "pnpm -w exec prettier --write frontend/",
    "format:check": "pnpm -w exec prettier --check frontend/",
    "preview": "vite preview",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
